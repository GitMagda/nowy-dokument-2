{
	"name": "future-collars",
	"displayName": "%displayName%",
	"description": "",
	"version": "1.1.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onUri",
		"workspaceContains:.futurecollars/**/*"
	],
	"main": "./dist/extension.js",
	"publisher": "LeanCode",
	"repository": "bitbucket:leancode-team/future-collars",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "cross-env TS_NODE_PROJECT=./tsconfig.webpack.json webpack --mode=production",
		"watch:webpack": "cross-env TS_NODE_PROJECT=./tsconfig.webpack.json webpack --mode=development --watch",
		"watch:tsc": "tsc --watch -p ./",
		"pretest": "npm run compile",
		"test": "echo No tests",
		"package": "vsce package"
	},
	"contributes": {
		"configuration": [
			{
				"title": "Future Collars",
				"properties": {
					"futurecollars.disableTheme": {
						"type": "boolean",
						"default": false,
						"description": "Disable setting VS Code theme on startup"
					},
					"futurecollars.loggingLevel": {
						"type": "string",
						"enum": [
							"None",
							"Fatal",
							"Error",
							"Warning",
							"Information",
							"Debug",
							"Trace"
						],
						"default": "Warning",
						"description": "The verbosity of logging. The Order is None < Fatal < Error < Warning < Information < Debug < Trace.",
						"scope": "resource"
					}
				}
			}
		],
		"commands": [
			{
				"command": "futurecollars.startAssignment",
				"title": "Start Assignment"
			}
		],
		"colors": [
			{
				"id": "futurecollars.basic.blue",
				"defaults": {
					"dark": "#0d88d3",
					"light": "#0d88d3",
					"highContrast": "#0d88d3"
				},
				"description": "Blue"
			},
			{
				"id": "futurecollars.basic.darkblue",
				"defaults": {
					"dark": "#1969b0",
					"light": "#1969b0",
					"highContrast": "#1969b0"
				},
				"description": "Dark blue"
			},
			{
				"id": "futurecollars.basic.lightblue",
				"defaults": {
					"light": "#66cef6",
					"dark": "#66cef6",
					"highContrast": "#66cef6"
				},
				"description": "Light blue"
			},
			{
				"id": "futurecollars.basic.grey",
				"defaults": {
					"dark": "#656263",
					"light": "#656263",
					"highContrast": "#656263"
				},
				"description": "Grey"
			},
			{
				"id": "futurecollars.basic.green",
				"defaults": {
					"dark": "#b3de45",
					"light": "#b3de45",
					"highContrast": "#b3de45"
				},
				"description": "Green"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "futurecollars.tests",
					"name": "%futurecollars.tests.name%",
					"when": "extensionIsActive"
				}
			]
		}
	},
	"devDependencies": {
		"@leancode/linting": "4.1.0-alpha.5",
		"@types/glob": "7.1.1",
		"@types/log4js": "2.3.5",
		"@types/mocha": "5.2.7",
		"@types/node": "12.7.8",
		"@types/vscode": "1.38.0",
		"@types/webpack": "4.39.1",
		"cross-env": "6.0.0",
		"fork-ts-checker-webpack-plugin": "1.5.0",
		"glob": "7.1.4",
		"mocha": "6.2.0",
		"simple-git": "2.20.1",
		"ts-loader": "6.1.2",
		"ts-node": "8.4.1",
		"tsconfig-paths-webpack-plugin": "3.2.0",
		"typescript": "3.7.4",
		"vsce": "1.71.0",
		"vscode-nls-dev": "3.3.1",
		"vscode-test": "1.2.0",
		"webpack": "4.41.0",
		"webpack-cli": "3.3.9"
	},
	"dependencies": {
		"junitxml-to-javascript": "1.1.3",
		"log4js": "6.3.0",
		"mobx": "5.15.1",
		"vscode-nls": "4.1.1"
	},
	"__metadata": {
		"id": "1a3181e7-4fb7-458b-ad95-16f56a850147",
		"publisherId": "86e483a0-3570-4767-944a-33a69926a70d",
		"publisherDisplayName": "LeanCode",
		"installedTimestamp": 1638612967471
	}
}